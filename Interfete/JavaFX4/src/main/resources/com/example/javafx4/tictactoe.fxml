<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.GridPane?>
<VBox alignment="CENTER" spacing="20.0" xmlns:fx="http://javafx.com/fxml"
      fx:controller="com.example.javafx4.TicTacToe">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
    </padding>

    <GridPane alignment="CENTER" hgap="5" vgap="5">
        <Button fx:id="button00" prefWidth="64" prefHeight="64" text="" GridPane.rowIndex="0" GridPane.columnIndex="0"/>
        <Button fx:id="button01" prefWidth="64" prefHeight="64" text="" GridPane.rowIndex="0" GridPane.columnIndex="1"/>
        <Button fx:id="button02" prefWidth="64" prefHeight="64" text="" GridPane.rowIndex="0" GridPane.columnIndex="2"/>

        <Button fx:id="button10" prefWidth="64" prefHeight="64" text="" GridPane.rowIndex="1" GridPane.columnIndex="0"/>
        <Button fx:id="button11" prefWidth="64" prefHeight="64" text="" GridPane.rowIndex="1" GridPane.columnIndex="1"/>
        <Button fx:id="button12" prefWidth="64" prefHeight="64" text="" GridPane.rowIndex="1" GridPane.columnIndex="2"/>

        <Button fx:id="button20" prefWidth="64" prefHeight="64" text="" GridPane.rowIndex="2" GridPane.columnIndex="0"/>
        <Button fx:id="button21" prefWidth="64" prefHeight="64" text="" GridPane.rowIndex="2" GridPane.columnIndex="1"/>
        <Button fx:id="button22" prefWidth="64" prefHeight="64" text="" GridPane.rowIndex="2" GridPane.columnIndex="2"/>
    </GridPane>
    <Label fx:id="winnerText"/>
    <Button fx:id="resetButton" text="Reset" onAction="#reset"/>
</VBox>
